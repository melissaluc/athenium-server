{
    "family": "athenium-backend",
    "executionRoleArn": "arn:aws:iam::ACCOUNT_NUMBER:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
      {
        "name": "athenium-container",
        "image": "YOUR_IMAGE_PLACEHOLDER",
        "cpu": 0,
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [],
        "environmentFiles": [
          {
            "value": "arn:aws:s3:::athenium-server-code-bucket/.env",
            "type": "s3"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/athenium-backend",
            "awslogs-region": "us-east-2",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "1024",
    "memory": "3072"
  }
  